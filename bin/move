#!/usr/bin/env ruby

require_relative '../lib/move.rb'
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
puts "Welcome to Tic Tac Toe!"
display_board(board)
puts "Where would you like to go?"
index = gets.strip

selected = input_to_index(index)
puts selected
board = move(board, selected, "X")
puts board
display_board(board)